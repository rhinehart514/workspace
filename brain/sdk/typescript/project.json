{
  "name": "@workspace/brain",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "brain/sdk/typescript/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:sdk"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc",
        "cwd": "brain/sdk/typescript"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm test",
        "cwd": "brain/sdk/typescript"
      }
    }
  }
}
