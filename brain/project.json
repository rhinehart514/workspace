{
  "name": "brain",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "brain",
  "projectType": "library",
  "tags": ["scope:shared", "type:brain"],
  "targets": {
    "build-ts": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd brain/sdk/typescript && npm run build"
      }
    },
    "reflect": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Run reflection agent'"
      }
    },
    "explore": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Run exploration agent'"
      }
    },
    "synthesize": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Run synthesis agent'"
      }
    },
    "curate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Run curator agent'"
      }
    },
    "network-report": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python -m brain.human.analysis.run_all",
        "cwd": "brain/sdk/python"
      }
    }
  }
}
